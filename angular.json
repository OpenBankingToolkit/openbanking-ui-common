{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gdpr": {
      "root": "projects/gdpr",
      "sourceRoot": "projects/gdpr/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/gdpr/tsconfig.lib.json",
            "project": "projects/gdpr/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gdpr/src/test.ts",
            "tsConfig": "projects/gdpr/tsconfig.spec.json",
            "karmaConfig": "projects/gdpr/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/gdpr/tsconfig.lib.json", "projects/gdpr/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
